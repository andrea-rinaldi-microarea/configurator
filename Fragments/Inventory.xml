    <ShortName name="MAGA" />
  </ShortNames>
  <Application container="Applications" name="ERP">
    <Module name="Inventory" />
    <Module name="Items" />
    <Module name="Business_CN">
      <Allow iso="CN" />
    </Module>
    <Module name="ItemCodes">
      <Allow iso="HU,PL" />
    </Module>
    <Module name="InventoryAccounting">
      <Allow iso="RO,RS,HR,PL,US,JP,CN,BG" />
