  </Application>
  <IncludeModulesPath path="AddOnsInventory" onselfactivated="true" />
  <IncludeModulesPath path="AddOnsItems" onselfactivated="true" />
  <IncludeModulesPath path="AddOnsPricePolicies" onselfactivated="true" />
  <IncludeModulesPath path="AddOnsInventoryChecking" onselfactivated="true" />
