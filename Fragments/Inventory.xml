    </Functionality>
    <Functionality name="FiscalABC">
      <Allow iso="IT,DE" />
    </Functionality>
    <Functionality name="ItemMarkup_IT">
      <Allow iso="IT" />
    </Functionality>
    <Functionality name="ItemMarkup_PL">
      <Allow iso="PL" />
